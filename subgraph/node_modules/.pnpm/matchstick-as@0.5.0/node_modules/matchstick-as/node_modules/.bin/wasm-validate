#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tnrdd/web3/flow-council/subgraph/node_modules/.pnpm/wabt@1.0.24/node_modules/wabt/bin/node_modules:/Users/tnrdd/web3/flow-council/subgraph/node_modules/.pnpm/wabt@1.0.24/node_modules/wabt/node_modules:/Users/tnrdd/web3/flow-council/subgraph/node_modules/.pnpm/wabt@1.0.24/node_modules:/Users/tnrdd/web3/flow-council/subgraph/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tnrdd/web3/flow-council/subgraph/node_modules/.pnpm/wabt@1.0.24/node_modules/wabt/bin/node_modules:/Users/tnrdd/web3/flow-council/subgraph/node_modules/.pnpm/wabt@1.0.24/node_modules/wabt/node_modules:/Users/tnrdd/web3/flow-council/subgraph/node_modules/.pnpm/wabt@1.0.24/node_modules:/Users/tnrdd/web3/flow-council/subgraph/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../wabt@1.0.24/node_modules/wabt/bin/wasm-validate" "$@"
else
  exec node  "$basedir/../../../../../wabt@1.0.24/node_modules/wabt/bin/wasm-validate" "$@"
fi
