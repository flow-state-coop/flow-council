#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tnrdd/web3/flow-council/subgraph/node_modules/.pnpm/gluegun@5.1.6_debug@4.3.4/node_modules/gluegun/bin/node_modules:/Users/tnrdd/web3/flow-council/subgraph/node_modules/.pnpm/gluegun@5.1.6_debug@4.3.4/node_modules/gluegun/node_modules:/Users/tnrdd/web3/flow-council/subgraph/node_modules/.pnpm/gluegun@5.1.6_debug@4.3.4/node_modules:/Users/tnrdd/web3/flow-council/subgraph/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tnrdd/web3/flow-council/subgraph/node_modules/.pnpm/gluegun@5.1.6_debug@4.3.4/node_modules/gluegun/bin/node_modules:/Users/tnrdd/web3/flow-council/subgraph/node_modules/.pnpm/gluegun@5.1.6_debug@4.3.4/node_modules/gluegun/node_modules:/Users/tnrdd/web3/flow-council/subgraph/node_modules/.pnpm/gluegun@5.1.6_debug@4.3.4/node_modules:/Users/tnrdd/web3/flow-council/subgraph/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../gluegun/bin/gluegun" "$@"
else
  exec node  "$basedir/../gluegun/bin/gluegun" "$@"
fi
